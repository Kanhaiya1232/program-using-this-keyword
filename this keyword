#include<iostream>
using namespace std;
class student
{
    public:
    int id;
    string name;
    void insert(int id,string name)
    {
    this->id=id;
    this->name=name;
    }
    void print()
    {
    cout<<id<<" "<<name<<" "<<endl;
    }
};
int main()
{
    student s;
    s.insert(10,"kanhaiya");
    s.print();
    return 0;
}

